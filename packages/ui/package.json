{
  "name": "@ui/design-system",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "src",
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "codegen": "panda codegen --clean",
    "build": "tsup src/index.ts --dts --format=esm,cjs --clean --dts --external @pandacss/dev --external @ui/style-system",
    "dev": "pnpm build --watch",
    "lint": "eslint src/",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "eslint-config-custom": "workspace:*",
    "react": "^18.1.0",
    "tsconfig": "workspace:*",
    "tsup": "^5.10.1",
    "typescript": "^5.2.2",
    "@pandacss/dev": "^0.15.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
